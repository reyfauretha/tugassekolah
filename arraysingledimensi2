package array;

public class SingleDimensi2 {
 public static void main (String[] args){
int [] jumlahhari;
jumlahhari = new int [7];
jumlahhari[0] = 31;
jumlahhari[1] = 28;
jumlahhari[2] = 31;
jumlahhari[3] = 30;
jumlahhari[4] = 31;
jumlahhari[5] = 30;
jumlahhari[6] = 31;
System.out.println("Bulan November memiliki" + jumlahhari[2] + " hari.");
 }   
}
