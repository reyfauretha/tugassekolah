package array;

public class pratikum2 {

    public static void main(String[] args){
    
        //int bilangan[5] = {1,2,3,4,5,6,7,8};
        int bilangan[] = {1, 2 ,3 ,4 ,5 ,6 ,7 ,8};
        for (int i = 0; i < bilangan.length; i++){
            System.out.print(bilangan[i]);
            System.out.print(" ");
        }
        
    }
}
